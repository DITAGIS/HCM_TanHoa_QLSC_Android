    <string name="sql_login">select *
					 from &table_sys_account; a
					 inner join &table_sys_role; r on r.id = a.Role where username = ? and password = ?</string>
    <string name="sql_login_imei">select *
					 from &table_sys_account; a
					 inner join &table_sys_role; r on r.id = a.Role where username = ? and password = ? and imei = ?</string>
